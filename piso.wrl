#VRML V2.0 utf8

DEF	concreto Transform {
	translation	0 -2 0
	children [
		 
		Inline {
			url "piso_base.wrl"
		}
		
	]
}

Transform {
	translation	-30 0 0
	children [
	  USE concreto
	]
}

Transform {
	translation	30 0 0
	children [
	  USE concreto
	]
}

Transform {
   translation 30 0 15
   children	[
	  USE concreto
   ]
}

Transform {
   translation -30 0 15
   children	[
	   USE concreto
   ]
}

Transform {
   translation 30 0 -15
   children	[
	  USE concreto
   ]
}

Transform {
   translation -30 0 -15
   children	[
	   USE concreto
   ]
}

Transform {
   translation 0 0 15
   children	[
	  USE concreto
   ]
}

Transform {
   translation 0 0 -15
   children	[
	   USE concreto
   ]
}


